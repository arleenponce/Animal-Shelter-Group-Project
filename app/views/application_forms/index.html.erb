<p id="notice"><%= notice %></p>

<h1>Listing Application Forms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Street address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Information</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @application_forms.each do |application_form| %>
      <tr>
        <td><%= application_form.name %></td>
        <td><%= application_form.phone %></td>
        <td><%= application_form.email %></td>
        <td><%= application_form.street_address %></td>
        <td><%= application_form.city %></td>
        <td><%= application_form.state %></td>
        <td><%= application_form.zip %></td>
        <td><%= application_form.information %></td>
        <td><%= link_to 'Show', application_form %></td>
        <td><%= link_to 'Edit', edit_application_form_path(application_form) %></td>
        <td><%= link_to 'Destroy', application_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Application form', new_application_form_path %>
